{
  "name": "egg-views",
  "version": "1.0.0",
  "description": "egg view plugin for all views in consolidate, like koa-views",
  "eggPlugin": {
    "name": "views",
    "dependencies": [
      "view"
    ]
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "egg-view",
    "egg-views",
    "consolidate"
  ],
  "dependencies": {
    "consolidate": "^0.15.1"
  },
  "devDependencies": {
    "autod": "^3.0.0",
    "autod-egg": "^1.0.0",
    "egg": "^2.0.0",
    "egg-bin": "^4.3.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.13.0",
    "ejs": "^2.5.8",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^5.1.0",
    "mz": "^2.7.0",
    "supertest": "^3.0.0",
    "vash": "^0.12.4",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
